| mvexpand techContact
| rename techContact AS title
| table title]
| eval last_successful_login=strftime(last_successful_login,"%Y-%m-%d %H:%M:%S")
| table title realname email last_successful_login
| rename title AS User realname AS Name email AS Email last_successful_login AS "Last Login"</query>
